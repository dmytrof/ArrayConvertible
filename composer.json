{
    "name": "dmytrof/array-convertible",
    "type": "library",
    "description": "Library helps to convert object to array",
    "keywords": ["array", "array convertible", "toArray"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3 || ^9.1",
        "slevomat/coding-standard": "^8.14",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\ArrayConvertible\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\ArrayConvertible\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit"
    }
}
